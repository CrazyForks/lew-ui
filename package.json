{
    "name": "lew-ui",
    "version": "1.1.9",
    "description": "A Component Library for Vue3.js.",
    "keywords": [
        "vue3",
        "components"
    ],
    "author": "lewkamtao",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build:noemit": "vite build --mode demo",
        "build": "vue-tsc --noEmit && vite build --mode demo",
        "build:package": "vue-tsc --noEmit && vite build --mode package",
        "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "format": "prettier --write './*.{js,ts,json,vue,css,scss}' './*/*.{js,ts,json,vue,css,scss}' './*/*/*.{js,ts,json,vue,css,scss}' './*/*/*/*.{js,ts,json,vue,css,scss}' './*/*/*/*/*.{js,ts,json,vue,css,scss}'",
        "test": "vitest",
        "test:coverage": "vitest --coverage"
    },
    "files": [
        "dist"
    ],
    "module": "./dist/lew.es.ts",
    "main": "./dist/lew.umd.ts",
    "exports": {
        ".": {
            "import": "./dist/lew.es.ts",
            "require": "./dist/lew.umd.ts"
        },
        "./dist/style.css": {
            "import": "./dist/style.css",
            "require": "./dist/style.css"
        }
    },
    "dependencies": {
        "@vicons/fluent": "^0.12.0",
        "@vicons/ionicons5": "^0.12.0",
        "@vueuse/core": "^9.1.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "tippy.js": "^6.3.7",
        "vue": "^3.2.25",
        "vue-router": "^4.0.13"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.182",
        "@types/node": "^17.0.35",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "@vicons/utils": "^0.1.4",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vitejs/plugin-vue-jsx": "^1.3.10",
        "@vue/test-utils": "^2.0.2",
        "c8": "^7.12.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^9.0.1",
        "jsdom": "^20.0.0",
        "prettier": "^2.6.2",
        "sass": "^1.52.1",
        "typescript": "^4.5.4",
        "vite": "^2.9.9",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-style-import": "^2.0.0",
        "vitest": "^0.21.1",
        "vue-tsc": "^0.34.7"
    }
}